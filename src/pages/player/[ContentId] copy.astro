---
export const prerender = false;

const { resource, id, title } = Astro.params;

import Video from "../../components/Video.astro";
import Inicio from "../../components/Iconos/Inicio.astro";
---

<section
  class="flex flex-col justify-around mx-auto px-8 bg-[#13151a] w-full min-h-screen py-8"
>
  <div class="flex flex-row gap-20 items-center">
    <a
      href="/"
      class="hover:scale-110 flex flex-row justify-start px-4 items-center gap-4 border-2 rounded-2xl w-fit py-4 transition-all duration-500 cursor-pointer mx-6 size-20"
    >
      <div class="hover:scale-110 text-orange-900 hover:text-white">
        <Inicio />
      </div>

      <span class="font-workSans text-xl text-white">Volver a Inicio</span>
    </a>

    <h1 class="font-workSans text-xl text-white">
      Titulo contenido:
    </h1>
  </div>

  <article>
    <Video />
  </article>
</section>
